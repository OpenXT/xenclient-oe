+        Emitted when a new access point is found by the device.
+    -->
+    <signal name="AccessPointAdded">
+        <arg name="access_point" type="o"/>
+        </signal>
+
+    <!--
+        AccessPointRemoved:
+        @access_point: The object path of the access point that has disappeared.
