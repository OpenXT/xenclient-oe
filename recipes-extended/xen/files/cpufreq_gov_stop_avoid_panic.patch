--- xen-4.6.5.orig/xen/drivers/cpufreq/cpufreq_ondemand.c
+++ xen-4.6.5/xen/drivers/cpufreq/cpufreq_ondemand.c
@@ -273,8 +273,11 @@ int cpufreq_governor_dbs(struct cpufreq_
         break;
 
     case CPUFREQ_GOV_STOP:
-        dbs_timer_exit(this_dbs_info);
-        dbs_enable--;
+        if (this_dbs_info->enable)
+        {
+            dbs_timer_exit(this_dbs_info);
+            dbs_enable--;
+        }
 
         break;
 
